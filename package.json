{
  "name": "gekko",
  "version": "0.8.0",
  "description": "A bitcoin trading bot for auto trading at various exchanges",
  "keywords": [
    "trading",
    "bot",
    "bitcoin",
    "TA",
    "finance"
  ],
  "scripts": {
    "test": "./node_modules/.bin/mocha test/*.js --recursive test -u tdd --reporter spec",
    "start": "node ./gekko.js --config config.js --ui",
    "start:secure": "NODE_ENV=production node ./gekko.js --config config.js --ui",
    "audit": "npm audit && npm audit --audit-level=moderate",
    "security-check": "npm audit --audit-level=high",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "author": "Mike van Rossum <mike@mvr.me>",
  "dependencies": {
    "async": "^3.2.4",
    "bcryptjs": "^2.4.3",
    "bitfinex-api-node": "^7.0.0",
    "co-fs": "^1.2.0",
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "d3": "^7.8.5",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "gekko": "0.0.9",
    "helmet": "^7.1.0",
    "humanize-duration": "^3.31.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.16.1",
    "koa-bodyparser": "^4.4.1",
    "koa-cors": "^0.0.16",
    "koa-logger": "^2.0.1",
    "koa-router": "^12.0.1",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "natural": "^6.10.4",
    "node-fetch": "^2.7.0",
    "opn": "^6.0.0",
    "promisify-node": "^0.5.0",
    "prompt-lite": "0.1.1",
    "relieve": "^2.1.3",
    "retry": "^0.13.1",
    "semver": "^7.5.4",
    "sentiment": "^5.0.2",
    "sqlite3": "^5.1.7",
    "stats-lite": "^2.2.0",
    "tiny-promisify": "^0.1.1",
    "toml": "^3.0.0",
    "web-push": "^3.6.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.2.0",
    "proxyquire": "^2.1.3",
    "sinon": "^17.0.1",
    "supertest": "^6.3.4",
    "ws": "^8.18.3"
  },
  "engines": {
    "node": ">=18"
  },
  "overrides": {
    "bluebird": "^3.7.2"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/askmike/gekko.git"
  }
}
